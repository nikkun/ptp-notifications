{
  "manifest_version": 2,
  "name": "PTP Notifications",
  "description": "Provides Chrome notifications for PTP messages",
  "version": "1.0.1",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "160": "icons/icon160.png",
    "256": "icons/icon256.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "permissions": [
    "http://passthepopcorn.me/",
    "https://passthepopcorn.me/",
    "notifications",
    "storage",
    "alarms",
    "tabs"
  ]
}
